{
  "manifest_version": 3,
  "name": "Hyper Captcha",
  "description": "하이퍼 캡챠 크롬 확장",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hiworks.png",
    "default_title": "Hyper Captcha"
  },
  "host_permissions": [
    "http://dev.hyperinfo.co.kr:12004/*",
    "https://dev.hyperinfo.co.kr/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://safind.scourt.go.kr/sf/mysafind.jsp*"
      ],
      "all_frames": true,
      "js": ["content-script.js"]
    }
  ]
}
